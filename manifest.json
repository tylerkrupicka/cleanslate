{
  "manifest_version": 2,

  "name": "Clean Slate",
  "description": "The facebook clean slate",
  "version": "0.0.0",
  "icons" : {
  },
  "content_scripts": [
    {
      "matches": ["http://www.facebook.com/*", "https://www.facebook.com/*"],
      "js": ["jquery.js", "script.js"]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
